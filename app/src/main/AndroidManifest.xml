<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.activities.artgames.mega_prototype2">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LevelChoose" />
        <activity android:name=".FirstLevel" />
        <activity android:name=".second_level" />
        <activity android:name=".ThirdLevel" />
        <activity android:name=".FourthLevel" />
        <activity android:name=".FifthLevel" />
        <activity android:name=".Disclamer" />
    </application>

</manifest>